{
  "manifest_version": 3,
  "name": "CrateDIGGAOG",
  "version": "1.0.0",
  "description": "3D crate digger visualization - Browse your record collection in 3D",
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "CrateDIGGAOG - 3D Record Browser"
  },
  "permissions": [],
  "web_accessible_resources": [
    {
      "resources": ["cratediggerDB.csv"],
      "matches": ["<all_urls>"]
    }
  ]
}